<!DOCTYPE html>
<html>
<head>
	<title>Experiment #1: LOAF TRIANGULATION</title>
	<!-- <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/agate.min.css"> -->
	<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/styles/zenburn.min.css">
	<link rel="stylesheet" href="/css/style.css">
</head>
<body>
	<h1>LOAF TRIANGULATION</h1>
	<button onclick="reset()">Reset</button>
	<button onclick="trilateration()">Trilateration</button>
	<button onclick="init()">Init</button>
	<button onclick="clear()">Clear screen</button>
	<pre><code class="json">{ message: "no action selected" }</code></pre>
	<script src="http://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/highlight.js/9.12.0/highlight.min.js"></script>
	<script type="text/javascript">
		hljs.initHighlightingOnLoad();
		function clear(){
			$('code').text('');
		}

		function reset(){
			$.ajax({
				url: "/reset"
			})
			.done(function(data){
				$('code').text(JSON.stringify(data, null, 4))
				$('pre code').each(function(i, block){
					hljs.highlightBlock(block);
				});
			});
		}

		function trilateration(){
			$.ajax({
				url: "/trilateration",
			})
			.done(function(data){
				$('code').text(JSON.stringify(data, null, 4));
				$('pre code').each(function(i, block){
					hljs.highlightBlock(block);
				});
			});
		}

		function init(){
			$.ajax({
				url: '/init'
			})
			.done(function(data){
				$('code').text(JSON.stringify(data, null, 4));
				$('pre code').each(function(i, block){	
					hljs.highlightBlock(block);
				});
			});
		}
	</script>
</body>
</html>